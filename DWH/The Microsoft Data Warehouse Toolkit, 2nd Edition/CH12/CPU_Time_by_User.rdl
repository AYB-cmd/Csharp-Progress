  <DataSets>
    <DataSet Name="SystemUsage">
      <Query>
        <DataSourceName>Localhost</DataSourceName>
        <CommandText>SELECT C.session_id, login_name, auth_scheme, connect_time, cpu_time,
		row_count, last_request_start_time, program_name, client_interface_name 
	FROM sys.dm_exec_connections C
		JOIN sys.dm_exec_sessions S
		ON C.session_id = S.session_id
	ORDER BY cpu_time desc;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="session_id">
          <DataField>session_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="login_name">
