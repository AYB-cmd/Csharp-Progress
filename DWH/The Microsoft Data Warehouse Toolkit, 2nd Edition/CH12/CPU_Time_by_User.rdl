          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="auth_scheme">
          <DataField>auth_scheme</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="connect_time">
          <DataField>connect_time</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="cpu_time">
          <DataField>cpu_time</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="row_count">
          <DataField>row_count</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="last_request_start_time">
