                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT --TOP 1000 
      h.[SalesOrderID]
      ,d.[SalesOrderDetailID]
      ,h.[RevisionNumber]
      ,h.[SalesOrderNumber]
      ,ISNULL(h.[PurchaseOrderNumber],'Unknown PO num') AS [PurchaseOrderNumber]
      ,h.[AccountNumber]
      ,h.[OrderDate]AS OrderDT
      ,h.[DueDate] AS DueDT
      ,h.[ShipDate] AS ShipDT
      ,d.[ProductID]
      ,d.[SpecialOfferID]
      ,h.[CustomerID]
      ,ISNULL(h.SalesPersonID, 0) AS SalesPersonID    -- Internet orders have no salesperson
      ,CASE WHEN h.OnlineOrderFlag = 1 THEN -3 ELSE -2 END AS OrderInfoID
      ,h.[Status]
      ,h.[OnlineOrderFlag]
      ,h.[SubTotal]
      ,h.[TaxAmt]
      ,h.[Freight]
      ,d.[OrderQty]
      ,d.[UnitPrice]
      ,d.[UnitPriceDiscount]
      ,ISNULL(c.EndOfDayRate, 1.0) AS EndOfDayRate
      ,ISNULL(c.ToCurrencyCode, 'USD') AS CurrencyCode
      ,cust.CustomerKey
  FROM [StageSalesOrderHeader] h 
  INNER JOIN StageSalesOrderDetail d ON (h.SalesOrderID=d.SalesOrderID)
  LEFT OUTER JOIN StageCurrencyRate c ON (c.CurrencyRateID=h.CurrencyRateID )
--Although I wanted this to be a pure "use SSIS Lookups" example, that technique just isn't a good idea for historical loads.
--Cust has type2 attributes (and data), and we need to pick up the correct historical row.
LEFT OUTER JOIN MDWT_2008R2.dbo.DimCustomer cust ON (h.CustomerID=cust.CustomerID AND h.OrderDate BETWEEN cust.RowStartDate and cust.RowEndDate)</property>
