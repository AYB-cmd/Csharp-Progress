  <DataSets>
    <DataSet Name="Subject_Areas">
      <Query>
        <DataSourceName>DW_Metadata_DB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DBKey">
            <Value>=Parameters!DBKey.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>Select SA.* from DW_Subject_Areas SA
JOIN DW_Database_Contents DC
ON SA.Subject_Area_Key = DC.Subject_Area_Key
JOIN DW_Databases DB 
ON DB.Database_Key = DC.Database_Key 
where DB.Database_Key=@DBKey</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Subject_Area_Key">
